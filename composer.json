{
    "name": "seferov/aws-bundle",
    "type": "symfony-bundle",
    "description": "Symfony wrapper of AWS SDK",
    "keywords": ["aws", "amazon web services"],
    "license": "MIT",
    "authors": [
        {
            "name": "Farhad Safarov",
            "homepage": "https://farhadsafarov.com"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/seferov/awsBundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "~2.3|~3.0",
        "aws/aws-sdk-php": "^3.19"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~3.2",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Seferov\\AwsBundle\\": ""
        }
    }
